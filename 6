#include <iostream>

using namespace std;

int main()
{
    int n;
    char x;
    cin>>x;
    n=int(x);
    if(n<=91&&n>=64)
    {
        cout<<"Litera mare";
    }
    else if(n>=97&&n<=123)
    {
        cout<<"Litera mica";
    }
    else
    {
        cout<<"Nu este litera";
    }
    return 0;
}
